	public static void main(String[] args) {
		String bracket = "1(2)(3)";
		Print.print(construct(bracket));
	}
	
	public static Node construct(String bracket) {
		
		while(true) {
			
		}
		
		
		
		
		
		if ((bracket.charAt(0) == ')') || (bracket.length() == 0)) return null;
		if (bracket.charAt(0) == '(') {
			
		}
		else {
			Node node = new Node(Character.getNumericValue(bracket.charAt(0)));
		}
			
	}
}
