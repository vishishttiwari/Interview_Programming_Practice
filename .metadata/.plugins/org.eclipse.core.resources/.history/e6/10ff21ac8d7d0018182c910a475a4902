package util;

import java.util.ArrayList;

public class Weighted_Edge {
	public Node from;	
	public Node to;
	public double weight;
	
	public Weighted_Edge(Node from, Node to, double weight) {
		this.from = from;
		this.to = to;
		this.weight = weight;
	}
}

class Weighted_Node {
	public int value;
	public ArrayList<WeightedEdge> list;
	
	public Weighted_Node(int value) {
		this.value = value;
		list = new ArrayList<WeightedEdge>();
	}
}
