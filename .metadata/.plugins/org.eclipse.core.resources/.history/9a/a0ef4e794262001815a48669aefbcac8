import java.util.Arrays;

public class Arrays_Sort_Java_Function {
	public static void main(String[] args) {
		int[] arr = {9,8,7,6,5,4,3,2,1,0,-1,-2};
		Print.print(arr);
		subarray_Sort(arr);
		Print.print(arr);
	}
	
	public static void simple_Sort(int[] arr) {
		Arrays.sort(arr);
	}
	
	public static void subarray_Sort(int[] arr) {
		Arrays.sort(arr, 2, 5);
	}
}
