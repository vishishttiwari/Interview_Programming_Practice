import java.util.*;

public class Implementing_Comparator {
	static void toBeSorted(ArrayList<int[]> arr) {
		Collections.sort(arr, new sortByAscending());
		Collections.sort(arr, new sortByDescending());
	}
	
	// There are child classes
	public static class sortByAscending implements Comparator<int[]> {
		public int compare(int[] a, int[] b) {
			return a[1] - b[1];
		}
	}
	
	public static class sortByDescending implements Comparator<int[]> {
		public int compare(int[] a, int[] b) {
			return b[1] - a[1];
		}
	}
}
