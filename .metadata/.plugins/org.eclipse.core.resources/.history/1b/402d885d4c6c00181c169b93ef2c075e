package rotation;

import util.Node;

public class Left_Rotation {
	public static Node left(Node root) {
		Node left = root.left;
		Node leftRight = root.left.right;
		
		left.right = y;
	}
}

/*
T1, T2, T3 and T4 are subtrees.
 	  z                                      y 
 	 / \                                   /   \
    y   T4      Right Rotate (z)          x      z
   / \          - - - - - - - - ->      /  \    /  \ 
  x  T3                               T1  T2  T3  T4
